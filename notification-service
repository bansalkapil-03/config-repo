spring:
  application:
    name: notification-service
  kafka:
    bootstrap-servers: 9092

server:
  port: 8084

eureka:
  client:
    service-url:
      defaultZone: http://localhost:8761/eureka
    register-with-eureka: true 
    fetch-registry: true 
  instance:
    prefer-ip-address: true
  
management:
  endpoints:
    web:
      exposure:
        include: "*"
